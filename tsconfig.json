{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"module": "NodeNext",
		"moduleResolution": "nodenext",
		"strictNullChecks": true,
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"allowImportingTsExtensions": true
	}
}
